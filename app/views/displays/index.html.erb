<%= link_to "Add and edit objects", "/admin" %>

<h2>Upcoming birthdays</h2>
<table>
  <% @birthdays.each do |event| %>
    <%= render partial: "/displays/individual_event", locals: {event: event} %>
  <% end %>
</table>
<h2>Outstanding wants and haves</h2>
<table>
  <% @wants_and_has.each do |event| %>
    <%= render partial: "/displays/individual_event", locals: {event: event} %>
  <% end %>
</table>
